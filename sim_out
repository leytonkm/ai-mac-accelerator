:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2009.vpi";
S_0x6160dcac5660 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x6160dcac57f0 .scope module, "testbench" "testbench" 3 3;
 .timescale -9 -12;
v0x6160dcadbd90_0 .var "a", 15 0;
v0x6160dcadbe70_0 .var "acc_in", 31 0;
v0x6160dcadbf10_0 .net "acc_out", 31 0, v0x6160dcadb880_0;  1 drivers
v0x6160dcadbfb0_0 .var "b", 15 0;
v0x6160dcadc050_0 .var "clk", 0 0;
v0x6160dcadc140_0 .var "rst", 0 0;
E_0x6160dca8a5d0 .event posedge, v0x6160dcadba20_0;
S_0x6160dca89cf0 .scope module, "dut" "mac_cell" 3 10, 4 3 0, S_0x6160dcac57f0;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /INPUT 16 "a";
    .port_info 3 /INPUT 16 "b";
    .port_info 4 /INPUT 32 "acc_in";
    .port_info 5 /OUTPUT 32 "acc_out";
v0x6160dcab25a0_0 .net "a", 15 0, v0x6160dcadbd90_0;  1 drivers
v0x6160dcab2ba0_0 .net "acc_in", 31 0, v0x6160dcadbe70_0;  1 drivers
v0x6160dcadb880_0 .var "acc_out", 31 0;
v0x6160dcadb940_0 .net "b", 15 0, v0x6160dcadbfb0_0;  1 drivers
v0x6160dcadba20_0 .net "clk", 0 0, v0x6160dcadc050_0;  1 drivers
v0x6160dcadbb30_0 .var "mult_result", 31 0;
v0x6160dcadbc10_0 .net "rst", 0 0, v0x6160dcadc140_0;  1 drivers
E_0x6160dcaadf30 .event posedge, v0x6160dcadbc10_0, v0x6160dcadba20_0;
    .scope S_0x6160dca89cf0;
T_0 ;
    %wait E_0x6160dcaadf30;
    %load/vec4 v0x6160dcadbc10_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x6160dcadbb30_0, 0;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x6160dcadb880_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x6160dcab25a0_0;
    %pad/u 32;
    %load/vec4 v0x6160dcadb940_0;
    %pad/u 32;
    %mul;
    %assign/vec4 v0x6160dcadbb30_0, 0;
    %load/vec4 v0x6160dcadbb30_0;
    %load/vec4 v0x6160dcab2ba0_0;
    %add;
    %assign/vec4 v0x6160dcadb880_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x6160dcac57f0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6160dcadc050_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x6160dcac57f0;
T_2 ;
    %delay 5000, 0;
    %load/vec4 v0x6160dcadc050_0;
    %inv;
    %store/vec4 v0x6160dcadc050_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x6160dcac57f0;
T_3 ;
    %vpi_call/w 3 24 "$dumpfile", "sim/waveform.vcd" {0 0 0};
    %vpi_call/w 3 25 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x6160dcac57f0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x6160dcadc140_0, 0, 1;
    %pushi/vec4 0, 0, 16;
    %store/vec4 v0x6160dcadbd90_0, 0, 16;
    %pushi/vec4 0, 0, 16;
    %store/vec4 v0x6160dcadbfb0_0, 0, 16;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x6160dcadbe70_0, 0, 32;
    %wait E_0x6160dca8a5d0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6160dcadc140_0, 0, 1;
    %wait E_0x6160dca8a5d0;
    %pushi/vec4 3, 0, 16;
    %store/vec4 v0x6160dcadbd90_0, 0, 16;
    %pushi/vec4 4, 0, 16;
    %store/vec4 v0x6160dcadbfb0_0, 0, 16;
    %pushi/vec4 10, 0, 32;
    %store/vec4 v0x6160dcadbe70_0, 0, 32;
    %wait E_0x6160dca8a5d0;
    %vpi_call/w 3 36 "$display", "Test 1: acc_out = %d (should be 22)", v0x6160dcadbf10_0 {0 0 0};
    %pushi/vec4 2, 0, 16;
    %store/vec4 v0x6160dcadbd90_0, 0, 16;
    %pushi/vec4 5, 0, 16;
    %store/vec4 v0x6160dcadbfb0_0, 0, 16;
    %load/vec4 v0x6160dcadbf10_0;
    %store/vec4 v0x6160dcadbe70_0, 0, 32;
    %wait E_0x6160dca8a5d0;
    %vpi_call/w 3 42 "$display", "Test 2: acc_out = %d (should be 32)", v0x6160dcadbf10_0 {0 0 0};
    %vpi_call/w 3 44 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
